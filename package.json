		"url": "https://github.com/FRickReich/base/issues"
	},
	"homepage": "https://github.com/FRickReich/base#readme",
	"dependencies": {
		"dotenv": "^8.1.0",
		"express": "^4.17.1",
		"mongoose": "^5.7.1",
		"ts-node": "^8.4.1"
	},
	"devDependencies": {
		"typescript": "^3.4.5",
		"nodemon": "^1.19.2",
		"@types/node": "^12.0.2",
		"@types/dotenv": "^6.1.1",
		"@types/express": "^4.17.1",
