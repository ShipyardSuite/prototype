{
	"name": "prototype",
	"version": "0.1.0",
	"description": "Shipyard showcase prototype",
	"main": "server/index.ts",
	"scripts": {
		"start": "npm run build:prod && npm run start:prod",
		"build:dev": "cd server/ && ts-node index.ts",
		"start:dev": "nodemon --watch 'server/**/*.ts' --exec 'ts-node' server/index.ts",
		"build:prod": "tsc -p .",
		"start:prod": "cd dist && node index.js"
	},
